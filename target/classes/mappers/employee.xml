<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.big_fish.oa.dao.EmployeeDao">
    <select id="selectById" resultType="com.big_fish.oa.entity.Employee" parameterType="Long">
       select * from adm_employee where employee_id = #{value}
    </select>
</mapper>